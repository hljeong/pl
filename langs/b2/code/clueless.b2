fn main() {
  b = alloc(8 * (5 + 6));
  printi(fib(7));
  print("\n");
  i = 0;
  while (i < 127) {
    printi(i);
    print("\n");
    i = i << 1 | 1;
  }
  if (i > 3) print("i > 3\n");
  if (i > 180) {
    print("i > 180\n");
  }
  hello();
}

fn fib(n) {
  if (n <= 1) return n;
  return fib(n - 2) + fib(n - 1);
}

fn hello() print("hello\n");

